    function callForWhatsapp(phoneNumber) {
//    alert('called.....');
        var uesrData = {};
        uesrData['phone'] = phoneNumber;
        uesrData['media'] = 'WA';


        $.ajax({
            url: "https://www.comingtrailer.com/MyLogin/chkMobileNumber/",
            type: "POST",
            data: uesrData,
            async: false,
            success: function (data, textStatus, jqXHR) {
//                        console.log(data.trim());
//                        alert('new-'+data.trim());
                if (data.trim() == '') {
                    //                        alert("The mobile number has been already registered!!!");
                    location.reload();
                } else {
